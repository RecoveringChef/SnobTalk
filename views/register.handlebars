    <div class="container">
        <!-- Material form register -->
        <div class="card">

            <h5 class="card-header info-color white-text text-center py-4 dusty-grass-gradient">
                <strong>Sign up</strong>
            </h5>

            <!--Card content-->
            <div class="card-body px-lg-5 pt-0">

                <!-- Form -->
                <form class="text-center" style="color: #f5f5f5;" action="#!">

                    <div class="form-row">
               
                        <div class="col">
                            <!-- User name -->
                            <div class="md-form">
                                <input type="text" id="formUserName" class="form-control">
                                <label for="materialRegisterFormLastName" style="color:#f5f5f5">Pick a user name</label>
                            </div>
                        </div>
                    </div>

                    <!-- E-mail -->
                    <div class="md-form mt-0">
                        <input type="email" id="formEmail" class="form-control">
                        <label for="materialRegisterFormEmail" style="color:#f5f5f5">E-mail</label>
                    </div>

                    <!-- Password -->
                    <div class="md-form">
                        <input type="password" id="formPassword" class="form-control"
                            aria-describedby="materialRegisterFormPasswordHelpBlock">
                        <label for="materialRegisterFormPassword" style="color:#f5f5f5">Password</label>
                        <small id="formPasswordHelpBlock" class="form-text text-muted mb-4" style="color:#f5f5f5">
                            At least 8 characters and 1 digit
                        </small>
                    </div>
                    <!-- Sign up button -->
                    <button class="btn btn-outline-info btn-rounded btn-block my-4 waves-effect z-depth-0" id="createUser"
                        type="submit">Sign up</button>

                    <hr>

                    <!-- Terms of service -->
                    <p>By clicking
                        <em>Sign up</em> you agree to our
                        <a href="" target="_blank">terms of service</a>

                </form>
                <!-- Form -->

            </div>

        </div>
    </div>
    <!-- Material form register -->
    <br>
<br>
<br>
<br>
</body>

